{
  "name": "game-of-life",
  "version": "1.0.0",
  "description": "Cellular automation game",
  "license": "MIT",
  "main": "src/run.js",
  "scripts": {
    "start": "nodemon src/run.js --exec babel-node",
    "test": "mocha --require babel-core/register"
  },
  "author": "Tom Austin",
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "env": {
      "production": {
        "comments": false
      },
      "development": {
        "sourceMaps": true
      }
    }
  },
  "engines": {
    "node": ">6.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "log-update": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "sinon": "^4.0.1"
  }
}
